<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RecyclerViewMultiHeader">
        <attr name="videoScale" format="float" />
        <attr name="viewState">
            <enum name="video" value="0" />
            <enum name="view_header" value="1" />
            <enum name="web" value="2" />
            <enum name="view_header_top" value="3" />
        </attr>
    </declare-styleable>
    <declare-styleable name="WebViewProxyScrollBar">
        <attr name="progressHeight" format="dimension" />
        <attr name="progressValue" format="float" />
        <attr name="progressColor" format="color" />
    </declare-styleable>
</resources>